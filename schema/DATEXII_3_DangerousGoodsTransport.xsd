          <xs:documentation>post office box number  as part of an address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="street" type="com:String" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>street name and number and as part of an address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="department" type="com:String" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>company department name or number</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="zipPostalCode" type="com:String" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>ZIP/Postal Code corresponding to the street address</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="regionName" type="com:String" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>name of the region within a country specific code to this address. Used to hold the state in US addresses. The code related to the name can be identified in the UNECE Recommendation N 16 - LOCODE</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cityName" type="com:String" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>name of the city. The code related to the name can be identified in the UNECE Recommendation N 16 - LOCODE</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryCode" type="com:String" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>ISO Country Codes ISO 3166-1/1998, UNECE Recommendation N 3 - Code for the Representation of Names of Countries</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="countryName" type="com:String" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>name of the country</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="_addressExtension" type="com:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
